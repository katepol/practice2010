	<td class="alt1" id="td_post_369" style="border-bottom:0px;background:#fff;">
	











		
		
			<!-- icon and title -->
			<div class="smallfont">
				
				<strong>Re:  Футбол.Новости!!!</strong>
			</div>
			<hr size="1" style="color:#fff; background-color:#fff" />
