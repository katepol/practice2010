	<td class="alt1" id="td_post_508" style="border-bottom:0px;background:#fff;">
	











		
		
			<!-- icon and title -->
			<div class="smallfont">
				<img class="inlineimg" src="http://forumishka.net/images/icons/icon1.gif" alt="Сообщение" border="0" />
				<strong>Новости</strong>
			</div>
			<hr size="1" style="color:#fff; background-color:#fff" />
